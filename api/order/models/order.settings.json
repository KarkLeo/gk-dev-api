{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "order",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "number": {
      "type": "uid"
    },
    "date": {
      "type": "datetime"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "created",
        "accepted",
        "paid",
        "sent",
        "received"
      ]
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "orders"
    },
    "delivery_info": {
      "type": "component",
      "repeatable": false,
      "component": "user.delivery-info"
    },
    "cart_item": {
      "type": "component",
      "repeatable": true,
      "component": "cart.cart-item"
    },
    "total_cost": {
      "type": "decimal"
    },
    "discount": {
      "type": "decimal"
    },
    "discounted_cost": {
      "type": "decimal"
    },
    "description": {
      "type": "text"
    }
  }
}
